a = float(input("subject 1: "))
b = float(input("subject 2: "))
c = float(input("subject 3: "))
d = float(input("subject 4: "))
e = float(input("subject 5: "))

average = (a+b+c+d+e)/5

if 90<=average<=100:
	grade = 'A'
elif 80<=average<90:
	grade = 'B'
elif 70 <=average <80:
	grade = 'C'
elif 60 <=average <70:
	grade = 'D'
else:
	grade = 'F'

print(f"Average Marks: {average:.2f}")
print(f"Grade: {grade}")
